      <attribute name="subnetMask" type="string" isCombinedKey="true" defaultValue="255.255.255.255" />
      <attribute name="domainName" type="string" isCombinedKey="true" />
      <attribute name="allowed" type="bool" defaultValue="false" />
    </collection>
  </sectionSchema>

